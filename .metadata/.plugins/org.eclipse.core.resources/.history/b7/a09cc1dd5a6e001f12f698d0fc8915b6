package testSuperKeyword;


class Animal{
	String color = "white";
}

class Dog extends Animal{
	String color = "Black";
	public void displayColor(){
		System.out.print(color);
	}
}

public class SuperKeyword {

	public static void main(String[] args) {
		Dog d = new Dog();
		d.displayColor();
		
		

	}

}
